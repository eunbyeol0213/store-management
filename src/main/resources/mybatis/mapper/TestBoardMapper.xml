<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mycompany.webapp.test.dao.ITestRepository">
<select id="getAllPosts" resultType="com.mycompany.webapp.test.vo.TestVO">
	SELECT
		postno,
		title,
		content,
		author,
		posttime
	FROM testboard
</select>
</mapper>